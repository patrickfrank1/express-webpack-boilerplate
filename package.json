{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate setup with webpack, express, babel, eslint, jest",
  "private": true,
  "main": "server.js",
  "browser": "./dist/index.html",
  "engines": {
    "node": "12.14.x",
    "npm": "6.13.x"
  },
  "scripts": {
    "test": "jest",
    "build:dev": "webpack-dev-server --open",
    "build:docs": "rm -rf documentation && ./node_modules/.bin/jsdoc -c jsdoc.config.js",
    "build:prod": "rm -rf dist && webpack -p --progress",
    "heroku-postbuild": "webpack -p --progress && npm run build:docs && npm run cleanup",
    "cleanup": "npm prune --production && rm -rf src test .eslintrc.js *.config.js README.md",
    "start": "node server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/patrickfrank1/express-webpack-boilerplate"
  },
  "author": "Patrick Frank",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/preset-env": "^7.8.4",
    "babel-loader": "^8.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.3",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.20.1",
    "file-loader": "^6.2.0",
    "html-loader": "^4.2.0",
    "html-webpack-plugin": "^5.5.0",
    "image-webpack-loader": "^8.1.0",
    "jest": "^25.1.0",
    "jsdoc": "^3.6.7",
    "mini-css-extract-plugin": "^2.7.2",
    "node-sass": "^4.13.1",
    "sass-loader": "^13.2.0",
    "terser-webpack-plugin": "^2.3.5",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1",
    "webpack-dev-server": "^3.10.3"
  },
  "dependencies": {
    "express": "^4.17.2",
    "regenerator-runtime": "^0.13.9"
  }
}
